<div class="booking-container">

  <% @bookings.each do |booking| %>
    <div class="booking">
      <h3>Booking Details</h3>
      <p><strong>Car Model:</strong> <%= booking.car.model %></p>
      <p><strong>User Name:</strong> <%= booking.user.name %></p>
      <p><%= link_to "Booking Details", booking_path(booking) %></p>
    </div>
  <% end %>
</div>
